def sum_of_digits(number):
    sum_of_even = 0
    sum_of_odd = 0
    for digit in number:
        if int(digit) % 2 == 0:
            sum_of_even += int(digit)
        else:
            sum_of_odd += int(digit)
    return sum_of_even, sum_of_odd

input_number = input()

sum_of_even, sum_of_odd = sum_of_digits(input_number)

print(f"Odd sum = {sum_of_odd}, Even sum = {sum_of_even} ")
