first_number_before = int(input())
second_number_before = int(input())

first_number_after = second_number_before
second_number_after = first_number_before

print("Before:")
print(f"a = {first_number_before}")
print(f"b = {second_number_before}")
print("After:")
print(f"a = {first_number_after}")
print(f"b = {second_number_after}")
